{
  "name": "magic-console",
  "main": "./lib/index",
  "version": "0.1.0",
  "description": "Interactive programming for Atom",
  "keywords": [
    "script"
  ],
  "repository": "https://github.com/gnestor/magic-console",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "docs": "gitbook build && open _book/index.html",
    "docs:watch": "gitbook serve",
    "postinstall": "npm install plugins",
    "apm": "apm install script fix-path"
  },
  "consumedServices": {
    "default-script-runtime": {
      "versions": {
        "0.1.0": "consumeDefaultRuntime"
      }
    },
    "provide-blank-runtime": {
      "versions": {
        "0.1.0": "consumeBlankRuntime"
      }
    }
  },
  "dependencies": {
    "react": ">=0.14.7",
    "react-deep-force-update": ">=2.0.1",
    "react-dom": ">=0.14.7",
    "redbox-react": ">=1.2.2"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.8",
    "babel-plugin-add-module-exports": ">=0.1.2",
    "babel-preset-es2015": ">=6.3.13",
    "babel-preset-react": ">=6.3.13",
    "babel-preset-stage-0": ">=6.3.13",
    "eslint": "^1.10.3",
    "gitbook-cli": "^1.0.1"
  }
}
